<!--logs.wxml-->
<view class="container log-list" style='padding: 0;margin: 0;'>
  <image style='display: block;width: 100%;height: 190px' mode='aspectFit' src='./../../imgs/banner_01.jpg'></image>
  <block wx:for="{{logs}}" wx:for-item="log" wx:key="index">
    <text class="log-item">{{index + 1}}. {{log}}</text>
  </block>
</view>
